{
  "name": "hextra",
  "version": "2.1.1",
  "description": "Professional-grade color manipulation suite with advanced luminance preservation",
  "keywords": [
    "image-processing",
    "color-manipulation",
    "hex-colors",
    "rgb-colors",
    "image-editor",
    "react",
    "material-ui",
    "jimp",
    "design-tools",
    "color-wheel"
  ],
  "author": {
    "name": "James Wolfe",
    "url": "https://github.com/wolfejam"
  },
  "private": true,
  "vercel": {
    "version": 2,
    "buildCommand": "npm run build",
    "cleanUrls": true
  },
  "engines": {
    "node": "22.12.0"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@kinde-oss/kinde-auth-react": "^3.0.23",
    "@mui/icons-material": "^5.14.16",
    "@mui/material": "^5.14.17",
    "jimp": "^0.22.10",
    "next": "^14.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "dev": "next dev",
    "build": "CI=false DISABLE_ESLINT_PLUGIN=true next build",
    "start": "next start",
    "generate-icons": "node scripts/generate-icons.js"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.23.5",
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "sharp": "^0.33.5"
  }
}
